{
    "name": "react-visual-regression-testing-example",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start:cra": "react-scripts start",
        "start:vite": "vite",
        "build:cra": "react-scripts build",
        "build:vite": "tsc && vite build",
        "preview:vite": "vite preview",
        "test:all": "react-scripts test --runInBand",
        "test:ci": "CI=true react-scripts test --modulePathIgnorePatterns=tests:visual --coverage",
        "eject": "react-scripts eject",
        "storybook": "start-storybook -p 6006 -s public",
        "build-storybook": "build-storybook -s public",
        "prepare": "husky install",
        "chromatic": "npx chromatic"
    },
    "dependencies": {
        "open-props": "^1.3.8",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-scripts": "5.0.0",
        "styled-components": "^5.3.3",
        "web-vitals": "^2.1.0"
    },
    "devDependencies": {
        "@storybook/addon-actions": "^6.4.19",
        "@storybook/addon-essentials": "^6.4.19",
        "@storybook/addon-interactions": "^6.4.19",
        "@storybook/addon-links": "^6.4.19",
        "@storybook/builder-webpack5": "^6.4.19",
        "@storybook/manager-webpack5": "^6.4.19",
        "@storybook/node-logger": "^6.4.19",
        "@storybook/preset-create-react-app": "^4.0.1",
        "@storybook/react": "^6.4.19",
        "@storybook/testing-library": "^0.0.9",
        "@testing-library/dom": "^8.11.3",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^12.0.0",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^27.0.1",
        "@types/jest-image-snapshot": "^4.3.1",
        "@types/jsdom-screenshot": "^3.2.1",
        "@types/node": "^16.7.13",
        "@types/react": "^17.0.20",
        "@types/react-dom": "^17.0.9",
        "@types/styled-components": "^5.1.23",
        "@types/styled-system": "^5.1.15",
        "@typescript-eslint/eslint-plugin": "^5.12.1",
        "@typescript-eslint/parser": "^5.12.1",
        "@vitejs/plugin-react": "^1.2.0",
        "chromatic": "^6.5.1",
        "eslint": "^8.9.0",
        "eslint-config-prettier": "^8.4.0",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "eslint-plugin-storybook": "^0.5.7",
        "husky": "^7.0.4",
        "jest-image-snapshot": "^4.5.1",
        "jsdom-screenshot": "^4.0.0",
        "lint-staged": "^12.3.4",
        "prettier": "^2.5.1",
        "storybook-builder-vite": "^0.1.21",
        "styled-system": "^5.1.5",
        "typescript": "^4.4.2",
        "vite": "^2.8.6",
        "webpack": "^5.69.1"
    },
    "lint-staged": {
        "*.{js,ts,tsx}": [
            "eslint --quiet --cache --fix"
        ],
        "*.{json,js,ts,jsx,tsx,html}": [
            "prettier --write"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "jest": {
        "coveragePathIgnorePatterns": [
            "src/styles",
            "src/stories",
            "tests:visual",
            "index.ts",
            "reportWebVitals.ts",
            "types.ts"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 90,
                "functions": 90,
                "lines": 90,
                "statements": 90
            }
        }
    }
}
